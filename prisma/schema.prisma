generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @unique @default(autoincrement())
  name      String
  email     String   @unique()
  active    Boolean  @default(true)
  lastLogin DateTime @map("last_login")

  @@map("users")
}
